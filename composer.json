{
    "name": "ctrl-f5/ctrlBlog",
    "description": "CtrlBlog",
    "license": "BSD-3-Clause",
    "keywords": [
        "ctrl-f5",
        "zf2"
    ],
    "homepage": "http://ctrl-f5.net/",
    "autoload": {
        "psr-0": {
            "CtrlBlog\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "http://github.com/ctrl-f5/ctrllib"
        },
        {
            "type": "git",
            "url": "http://github.com/ctrl-f5/ctrlAuth"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "dev-master",
        "ctrl-f5/ctrllib": "dev-master",
        "ctrl-f5/ctrlAuth": "dev-master"
    },
    "extra": {
        "installer-paths": {
            "public/js/vendor/{$vendor}/{$name}/": ["ctrl-f5/ctrljs"]
        }
    }
}
